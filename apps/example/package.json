{
    "name": "@stuntman/example",
    "private": "true",
    "version": "0.1.2",
    "description": "Stuntman - HTTP proxy / mock shared types and utils",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/andrzej-woof/stuntman.git"
    },
    "homepage": "https://github.com/andrzej-woof/stuntman#readme",
    "bugs": {
        "url": "https://github.com/andrzej-woof/stuntman/issues"
    },
    "scripts": {
        "test:example": "testcafe chrome **/*.testcafe.ts --app 'ts-node ./src/index.ts' --app-init-delay 3000",
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' ./src/index.ts",
        "clean": "rm -fr dist",
        "build": "tsc",
        "lint": "prettier --check . && eslint . --ext ts",
        "lint:fix": "prettier --write ./src && eslint ./src --ext ts --fix"
    },
    "keywords": [
        "proxy",
        "mock",
        "http",
        "https",
        "server",
        "api",
        "e2e",
        "development",
        "rest",
        "gql",
        "end-to-end",
        "testing",
        "qa",
        "automated-testing",
        "stub",
        "functional"
    ],
    "author": "Andrzej Pasterczyk",
    "license": "MIT",
    "dependencies": {
        "@stuntman/client": "workspace:^0.1.2",
        "@stuntman/server": "workspace:^0.1.2",
        "@stuntman/shared": "workspace:^0.1.2",
        "express": "5.0.0-beta.1",
        "express-session": "1.17.3",
        "node-fetch": "2",
        "pug": "3.0.2",
        "testcafe": "2.3.1",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@types/express": "4.17.17",
        "@types/express-session": "1.17.6",
        "@types/node-fetch": "2.6.2",
        "@types/uuid": "9.0.0"
    }
}
